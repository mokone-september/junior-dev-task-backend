{
  "version": 2,
  "builds": [
    {
      "src": "api/sort-characters.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/sort-characters",
      "dest": "/api/sort-characters",
      "methods": ["POST"]
    },
    {
      "src": "/api/health",
      "dest": "/api/sort-characters",
      "methods": ["GET"]
    }
  ]
}